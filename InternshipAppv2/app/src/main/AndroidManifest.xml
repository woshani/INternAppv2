<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.shay.incursio.internshipappv2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:noHistory="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="@android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".companyMenu" />
        <activity android:name=".studentMenu" />
        <activity android:name=".newVacancy" />
        <activity android:name=".viewVacancy" />
        <activity android:name=".viewVacancyStud" />
        <activity android:name=".CommitteMenu" />
        <activity android:name=".viewVacancyCommitte" />
        <activity android:name=".registerCompany" />
        <activity android:name=".viewApplicationCom" />
        <activity android:name=".viewApplicationStudent" />
        <activity android:name=".manageStudent"></activity>
    </application>

</manifest>